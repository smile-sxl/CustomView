<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gizwits.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewSlide.ViewSlideActivity" />
        <activity android:name=".ViewSlide.LayoutSlideActivity" />
        <activity android:name=".ViewSlide.OffsetSlideActivity" />
        <activity android:name=".ViewSlide.LayoutParamsSlideActivity" />
        <activity android:name=".ViewSlide.ScrollSlideActivity" />
        <activity android:name=".ViewSlide.ScrollerSlideActivity" />
    </application>

</manifest>